<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="714.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.ubbcluj.cs.map.lab7.controller.UserController">
   <children>
      <TableView fx:id="userTableView" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="524.0">
        <columns>
          <TableColumn fx:id="firstName" prefWidth="75.0" text="firstName" />
            <TableColumn fx:id="lastName" prefWidth="75.0" text="lastName" />
            <TableColumn fx:id="email" prefWidth="75.0" text="email" />
            <TableColumn fx:id="password" prefWidth="75.0" text="password" />
        </columns>
      </TableView>
      <Button layoutX="575.0" layoutY="188.0" mnemonicParsing="false" onAction="#onPressDelete" prefHeight="25.0" prefWidth="98.0" text="DELETE USER" />
      <TextField fx:id="firstNameTextField" layoutX="14.0" layoutY="227.0" promptText="firstName" />
      <TextField fx:id="passwordTextField" layoutX="14.0" layoutY="320.0" promptText="password" />
      <TextField fx:id="emailTextField" layoutX="14.0" layoutY="289.0" promptText="email" />
      <TextField fx:id="lastNameTextField" layoutX="14.0" layoutY="258.0" promptText="lastName" />
      <Button layoutX="178.0" layoutY="320.0" mnemonicParsing="false" onAction="#onPressAdd" prefHeight="25.0" prefWidth="98.0" text="ADD USER" />
   </children>
</AnchorPane>
