<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.ubbcluj.cs.map.lab82nd.LoginController">
   <children>
      <TextField fx:id="emailTextField" layoutX="226.0" layoutY="120.0" promptText="email" />
      <PasswordField fx:id="passwordTextField" layoutX="226.0" layoutY="168.0" promptText="password" />
      <Button layoutX="320.0" layoutY="214.0" mnemonicParsing="false" onAction="#onPressLogIn" text="LOG IN" />
      <Button layoutX="226.0" layoutY="214.0" mnemonicParsing="false" onAction="#onPressCancel" text="CANCEL" />
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#onPressBack" prefHeight="25.0" prefWidth="160.0" text="BACK" />
   </children>
</AnchorPane>
