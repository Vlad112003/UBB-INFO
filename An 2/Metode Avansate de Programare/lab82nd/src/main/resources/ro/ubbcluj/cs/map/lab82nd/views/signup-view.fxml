<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.ubbcluj.cs.map.lab82nd.SignUpController">
   <children>
      <TextField fx:id="firstNameField" layoutX="227.0" layoutY="101.0" promptText="firstName" />
      <TextField fx:id="lastNameField" layoutX="227.0" layoutY="132.0" promptText="lastName" />
      <TextField fx:id="emailField" layoutX="227.0" layoutY="164.0" promptText="email" />
      <PasswordField fx:id="passwordField" layoutX="227.0" layoutY="200.0" promptText="password" />
      <Button layoutX="314.0" layoutY="240.0" mnemonicParsing="false" onAction="#onPressSignup" text="SIGN UP" />
      <Button layoutX="227.0" layoutY="240.0" mnemonicParsing="false" onAction="#onPressCancel" prefHeight="25.0" prefWidth="62.0" text="CANCEL" />
      <Button layoutX="14.0" layoutY="361.0" mnemonicParsing="false" onAction="#onPressBack" prefHeight="25.0" prefWidth="160.0" text="BACK" />
   </children>
</AnchorPane>
