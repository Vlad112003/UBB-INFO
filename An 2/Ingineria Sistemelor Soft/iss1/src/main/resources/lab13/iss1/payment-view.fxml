<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<!--        fx:controller="lab13.iss1.PaymentView"-->


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab13.iss1.PaymentController">
   <children>
      <TableView layoutX="27.0" layoutY="38.0" prefHeight="200.0" prefWidth="370.0" fx:id="orderTable">
        <columns>
          <TableColumn prefWidth="130.0" text="USER" fx:id="userColumn"/>
          <TableColumn prefWidth="130.0" text="GAME" fx:id="gameColumn"/>
          <TableColumn minWidth="0.0" prefWidth="108.0" text="STATUS" fx:id="statusColumn"/>
        </columns>
      </TableView>
      <Button layoutX="492.0" layoutY="351.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="84.0" text="EXIT" fx:id="exitButton" onAction="#onPressExit"/>
      <Button layoutX="27.0" layoutY="264.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="APPROVE" fx:id="approveButton" onAction="#onPressApprove"/>
      <Button layoutX="258.0" layoutY="264.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="139.0" text="DECLINE" fx:id="declineButton" onAction="#onPressDecline"/>
   </children>
</AnchorPane>
