<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<!--        fx:controller="lab13.iss1.LoginView"-->

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab13.iss1.LoginController">
   <children>
      <TextField fx:id="emailField" layoutX="98.0" layoutY="144.0" prefHeight="25.0" prefWidth="405.0" promptText="email" />
      <PasswordField fx:id="passwordField" layoutX="98.0" layoutY="184.0" prefHeight="25.0" prefWidth="405.0" promptText="password" />
      <Button fx:id="loginButton" layoutX="190.0" layoutY="232.0" mnemonicParsing="false" onAction="#onPressLogin" prefHeight="25.0" prefWidth="220.0" text="LOG IN" />
      <Button fx:id="exitButton" layoutX="471.0" layoutY="361.0" mnemonicParsing="false" onAction="#onPressExit" prefHeight="25.0" prefWidth="115.0" text="EXIT" />
   </children>
</AnchorPane>
